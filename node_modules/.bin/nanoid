#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/xampp/htdocs/Website_StoneScissorPaper/node_modules/nanoid/bin/node_modules:/mnt/c/xampp/htdocs/Website_StoneScissorPaper/node_modules/nanoid/node_modules:/mnt/c/xampp/htdocs/Website_StoneScissorPaper/node_modules:/mnt/c/xampp/htdocs/node_modules:/mnt/c/xampp/node_modules:/mnt/c/node_modules:/mnt/c/xampp/htdocs/Website_StoneScissorPaper/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/xampp/htdocs/Website_StoneScissorPaper/node_modules/nanoid/bin/node_modules:/mnt/c/xampp/htdocs/Website_StoneScissorPaper/node_modules/nanoid/node_modules:/mnt/c/xampp/htdocs/Website_StoneScissorPaper/node_modules:/mnt/c/xampp/htdocs/node_modules:/mnt/c/xampp/node_modules:/mnt/c/node_modules:/mnt/c/xampp/htdocs/Website_StoneScissorPaper/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nanoid/bin/nanoid.cjs" "$@"
else
  exec node  "$basedir/../nanoid/bin/nanoid.cjs" "$@"
fi
